{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "UiDevServerUrl": "https://localhost:4201",
  "ReverseProxy": {
    "Routes": {
      "downstreamapiroute": {
        "ClusterId": "clusterdownstreamapicert",
        "AuthorizationPolicy": "Default",
        "Match": {
          "Path": "WeatherForecast"
        }
      },
      "assets": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "assets/{**catch-all}"
        }
      },
      "angularfsdev": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "@fs/{**catch-all}"
        }
      },
      "angularngdev": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "@ng/{**catch-all}"
        }
      },
      "vitedev": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "@vite/{**catch-all}"
        }
      },
      "wssvite": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "/",
          "QueryParameters": [
            {
              "Name": "token",
              "Mode": "Exists"
            }
          ]
        }
      },
      "routealljs": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{nomatterwhat}.js"
        }
      },
      "routeallcss": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{nomatterwhat}.css"
        }
      },
      "webpacklazyloadingsources": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "/src_{nomatterwhat}_ts.js"
        }
      },
      "webpacknodesrcmap": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "/{nomatterwhat}.js.map"
        }
      },
      "mediastreamroute": {
        "ClusterId": "clustermediastream",
        "AuthorizationPolicy": "Default",
        "Match": {
          "Path": "video/{**catch-all}"
        }
      },
      "bootstrap-iconswoff2": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "bootstrap-icons.woff2"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "HttpClient": {
          "SslProtocols": [
            "Tls12"
          ]
        },
        "Destinations": {
          "cluster1/destination1": {
            "Address": "https://localhost:4201/"
          }
        }
      },
      "clusterdownstreamapicert": {
        "HttpClient": {
          "SslProtocols": [
            "Tls12"
          ]
        },
        "Destinations": {
          "clusterdownstreamapicert/destination1": {
            "Address": "https://localhost:5007/"
            //"Address": "https://azurecertauth20201108214641.azurewebsites.net/"
          }
        }
      }
    }
  }
}
